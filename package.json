  "devDependencies": {
    "eslint": "^3.9.0",
    "eslint-plugin-immutable": "*",
    "istanbul": "^0.4.5",
    "tape": "^4.6.2"
  },
  "dependencies": {
    "ramda": "^0.22.1",
    "sanctuary-def": "^0.7.0"
  }
}
