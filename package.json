{
  "name": "sum-type",
  "version": "0.4.2",
  "description": "Descendant of paldepind/union-type with more descriptive error messages",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublish": "npm test",
    "test": "npm run test:test && npm run test:coverage && npm run test:lint",
