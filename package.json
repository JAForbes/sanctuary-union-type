{
  "name": "sum-type",
  "version": "0.11.1",
  "description": "Descendant of paldepind/union-type with more descriptive error messages",
  "main": "lib/simple.js",
  "module": "lib/index.js",
  "jsnext:main": "lib/index.js",
  "scripts": {
    "compile": "buble lib/index.js > index.js",
    "prepublish": "npm run compile && npm test && npm run dist",
    "test": "run-s test:**",
    "test:compile": "npm run compile",
    "test:test": "istanbul cover test/test.js",
    "test:coverage": "istanbul check-coverage --statements 100 --functions 100 --lines 100 --branches 100",
    "test:lint": "eslint --max-warnings 0 -c .eslintrc lib/index.js test",
    "dist": "browserify lib/simple.js --standalone SumType -p tinyify > sumtype.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/JAForbes/sum-type.git"
  },
  "license": "MIT",
  "devDependencies": {
    "browserify": "^14.3.0",
    "buble": "^0.15.1",
    "coveralls": "^2.13.1",
    "eslint": "^3.9.0",
    "eslint-plugin-immutable": "*",
    "istanbul": "^0.4.5",
    "npm-run-all": "^4.0.2",
    "sanctuary": "^0.12.2",
    "sanctuary-def": "^0.9.0",
    "static-sum-type": "^0.11.0",
    "tape": "^4.6.2",
    "tinyify": "^2.3.0",
    "uglify-js": "^2.8.22"
  },
  "peerDependencies": {
    "sanctuary-def": "^0.9.0"
  },
  "dependencies": {
    "ramda": "^0.25.0"
  }
}
